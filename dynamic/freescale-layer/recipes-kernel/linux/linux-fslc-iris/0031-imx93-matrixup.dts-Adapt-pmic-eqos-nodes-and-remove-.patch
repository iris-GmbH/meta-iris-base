+			MX93_PAD_SD3_CMD__GPIO3_IO21                            0x31e /* E_NRST */
+			MX93_PAD_SD3_DATA0__GPIO3_IO22                          0x31e /* E_INT */
 		>;
 	};
 
@@ -546,12 +442,6 @@ MX93_PAD_GPIO_IO29__LPI2C3_SCL			0x40000b9e
 		>;
 	};
 
-	pinctrl_pcal6524: pcal6524grp {
