 examples/www/v2/images/favicon.png            | Bin 959 -> 988 bytes
 examples/www/v2/images/logo.png               | Bin 832 -> 5344 bytes
 examples/www/v2/index.html                    |   2 +-
 .../SourceSansPro_Light_latin_ext.woff2       | Bin 0 -> 20264 bytes
 .../SourceSansPro_Semibold_latin_ext.woff2    | Bin 0 -> 20320 bytes
 6 files changed, 75 insertions(+), 1 deletion(-)
 create mode 100644 examples/www/v2/css/override.css
 create mode 100644 examples/www/v2/webfonts/SourceSansPro_Light_latin_ext.woff2
 create mode 100644 examples/www/v2/webfonts/SourceSansPro_Semibold_latin_ext.woff2

diff --git a/examples/www/v2/css/override.css b/examples/www/v2/css/override.css
new file mode 100644
index 00000000..89040e75
--- /dev/null
+++ b/examples/www/v2/css/override.css
@@ -0,0 +1,74 @@
+/*!
+ * Overrides from Iris GmbH & Co. KG
+ * 
+ */
+ @font-face {
+  font-family: 'Source Sans Pro';
+  font-weight: 300;
+  src: url(../webfonts/SourceSansPro_Light_latin_ext.woff2) format('woff2');
+}
+@font-face {
+  font-family: 'Source Sans Pro';
+  font-weight: 600;
+  src: url(../webfonts/SourceSansPro_Semibold_latin_ext.woff2) format('woff2');
+}
+
+ body {
+  background-attachment: fixed;
+  background-image: radial-gradient(farthest-corner at 20px 20px, rgb(80,80,80) 0%, black 100%);
+  background-position: center;
+  background-repeat: no-repeat;
+  background-size: cover;
+  background-color: black;
+  font-family: 'Source Sans Pro', sans-serif !important;
+}
+
+a {
+  color: #0090FF;
+}
+.navbar {
+  background: #000000 !important;
+  height: 70px;
+}
+
+.navbar-brand img {
+  width: 100px;
+}
+
+.nav-link {
+  border: 2px solid #e20030;
+  border-radius: 5px;
+  background: transparent;
+  color: #fff !important;
+  line-height: 0.7rem;
+  text-transform: uppercase;
+  font-size: 0.75rem !important;
+  font-weight: 600;
+  height: 1.8rem !important;
+  padding: 6px 20px !important;
+}
+
+.nav-link:hover {
+  color: #e20030 !important;
+}
+
+.container {
+  max-width: 96% !important;
+}
+.card {
+  background: #323232 !important;
+  /*color: white;*/
+}
+.jumbotron {
+  background: #323232 !important;
+  color: #ffffff;
+}
+
+.card-header,
+.card-body {
+  color: #ffffff;
+}
+
+.dropzone  {
+  color: #000000;
+}
\ No newline at end of file
